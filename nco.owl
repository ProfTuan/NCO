        <rdfs:subClassOf rdf:resource="http://w3id.org/nco#NCO_0000110"/>
        <rdfs:label>repeat request</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/nco#NCO_0000115 -->

    <owl:Class rdf:about="http://w3id.org/nco#NCO_0000115">
